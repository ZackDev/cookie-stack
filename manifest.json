{
  "manifest_version": 2,
  "name": "Cookie Stack",
  "version": "2021-03-23",
  "homepage_url": "https://github.com/ZackDev/cookie-stack",
  "permissions": [
    "<all_urls>",
    "cookies",
    "storage"
  ],
  "background": {
    "scripts": ["/zackdev/background.js"]
  },
  "browser_action" : {
    "default_icon": {
      "16": "/zackdev/favicon-16x16.png"
    },
    "default_title": "Cookie Stack",
    "default_popup": "/zackdev/popup.html",
    "browser_style": true
  }
}
