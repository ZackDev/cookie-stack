{
  "manifest_version": 2,
  "name": "Cookie Stack",
  "version": "0.15",
  "homepage_url": "https://github.com/ZackDev/cookie-stack",
  "description": "Manages cookies. Includes automatic deletion based on filters.",
  "permissions": [
    "<all_urls>",
    "cookies",
    "downloads",
    "storage"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "/icons/cookie-stack-logo-16x16.png"
    },
    "default_title": "Cookie Stack",
    "default_popup": "popup.html",
    "browser_style": true,
    "chrome_style": true
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true,
    "open_in_tab": true
  },
  "icons": {
    "48": "/icons/cookie-stack-logo-48x48.png",
    "96": "/icons/cookie-stack-logo-96x96.png"
  }
}
