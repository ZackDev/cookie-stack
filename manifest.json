{
  "manifest_version": 2,
  "name": "Cookie Stack",
  "version": "0.6",
  "version_name": "2023-02-07",
  "homepage_url": "https://github.com/ZackDev/cookie-stack",
  "permissions": [
    "<all_urls>",
    "cookies",
    "storage"
  ],
  "background": {
    "page": "/zackdev/background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "/zackdev/icons/favicon-16x16.png"
    },
    "default_title": "Cookie Stack",
    "default_popup": "/zackdev/popup.html",
    "browser_style": true,
    "chrome_style": true
  },
  "options_ui": {
    "page": "/zackdev/options.html",
    "browser_style": true,
    "chrome_style": true
  }
}